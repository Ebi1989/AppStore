
@model ProductsListViewModel
<h1>Wellcome to ASP.NET Core 06</h1>

@if (Model != null)
    @foreach (var item in Model.ProductPagedData.Data)
    {
        @await Html.PartialAsync("ProductSummary", item)
    }
<div page-model="Model.ProductPagedData.PageInfo" page-url-category="Model.CurrentCategory" page-action="Index" page-classes-enabled="true" page-class="btn" page-class-normal="btn-outline-dark" page-class-selected="btn-success" class="btn-group pull-right m-1"></div>
